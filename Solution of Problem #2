#include<stdio.h>
#include<math.h>
int main()
{
    double m1=3.0, wl1=530.0, theta1=65.0, d, Theta, m2=2.0, wl2=700.0, theta2;

    theta1*=(3.1416/180.0);
    Theta=sin(theta1);
    d=(m1*wl1)/Theta;

    theta2=(asin((m2*wl2)/d))*(180.0/3.1416);
    printf("The angle of red light is: %lf degree\n", theta2);
}
